{
  "__comment": "Modified from Quark",
  "multipart": [
    {
      "when": { "enabled": true },
      "apply": { "model": "aforce:block/active_force_tube" }
    },
    {
      "when": { "north": true },
      "apply": { "model": "aforce:block/pipe_side" }
    },
    {
      "when": { "south": true },
      "apply": { "model": "aforce:block/pipe_side", "y": 180 }
    },
    {
      "when": { "east": true },
      "apply": { "model": "aforce:block/pipe_side", "y": 90 }
    },
    {
      "when": { "west": true },
      "apply": { "model": "aforce:block/pipe_side", "y": 270 }
    },
    {
      "when": { "down": true },
      "apply": { "model": "aforce:block/pipe_side", "x": 90 }
    },
    {
      "when": { "up": true },
      "apply": { "model": "aforce:block/pipe_side", "x": 270 }
    },
    {
      "when": {
        "north": false
      },
      "apply": { "model": "aforce:block/pipe_center_side" }
    },
    {

      "when": {
        "south": false
      },
      "apply": { "model": "aforce:block/pipe_center_side", "y": 180 }
    },
    {

      "when": {
        "east": false
      },
      "apply": { "model": "aforce:block/pipe_center_side", "y": 90 }
    },
    {

      "when": {
        "west": false
      },
      "apply": { "model": "aforce:block/pipe_center_side", "y": 270 }
    },
    {

      "when": {
        "down": false
      },
      "apply": { "model": "aforce:block/pipe_center_side", "x": 90 }
    },
    {

      "when": {
        "up": false
      },
      "apply": { "model": "aforce:block/pipe_center_side", "x": 270 }
    }
  ]
}
